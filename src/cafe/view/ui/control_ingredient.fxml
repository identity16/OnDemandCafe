<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListCell?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root type="javafx.scene.control.ListCell" xmlns="http://javafx.com/javafx/8.0.5" xmlns:fx="http://javafx.com/fxml">
	<graphic>
		<HBox alignment="CENTER" minWidth="575.0" prefHeight="62.0" prefWidth="575.0" maxWidth="575.0" spacing="20.0">
			<Label fx:id="nameLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="62.0" prefWidth="503.0" style="-fx-background-color: rgb(224, 224, 224);" text="Ingredient" textAlignment="CENTER">
            <font>
               <Font name="SansSerif Bold" size="24.0" />
            </font></Label>
			<Button fx:id="deleteBtn" alignment="CENTER" prefHeight="44.0" prefWidth="44.0" style="-fx-background-color: red;" text="-" textAlignment="CENTER" textFill="WHITE">
            <font>
               <Font name="SansSerif Bold" size="23.0" />
            </font></Button>
		</HBox>
	</graphic>
</fx:root>

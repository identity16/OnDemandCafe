<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefHeight="300.0" prefWidth="1090.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cafe.controller.AddBaseDialogController">
   <children>
      <ScrollPane prefHeight="250.0" prefWidth="1090.0" vbarPolicy="NEVER">
         <content>
            <TilePane prefHeight="223.0" prefWidth="1088.0" fx:id="ingredientPane"/>
         </content>
      </ScrollPane>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="44.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="135.0" style="-fx-background-color: rgb(52, 152, 219)rgb(52, 152, 219); -fx-background-radius: 20;" text="ADD" textFill="WHITE" fx:id="addBtn">
               <font>
                  <Font name="SansSerif Bold" size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="135.0" style="-fx-background-color: rgb(230, 230, 230); -fx-background-radius: 20;" text="CANCEL" textFill="#838383" fx:id="cancelBtn">
               <font>
                  <Font name="SansSerif Bold" size="15.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
